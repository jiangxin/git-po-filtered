#, c-format
msgid "Huh (%s)?"
msgstr "O quê (%s)?"

msgid "could not read index"
msgstr "incapaz de ler cenário"

msgid "binary"
msgstr "binário"

msgid "nothing"
msgstr "nada"

msgid "unchanged"
msgstr "inalterado"

msgid "Update"
msgstr "Atualizar"

#, c-format
msgid "could not stage '%s'"
msgstr "incapaz de encenar '%s'"

msgid "could not write index"
msgstr "incapaz de escrever cenário"


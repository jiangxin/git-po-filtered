#: builtin/push.c
msgid "control recursive pushing of submodules"
msgstr "điều khiển việc đẩy lên (push) đệ qui của mô-đun-con"

#: builtin/push.c
msgid "use thin pack"
msgstr "tạo gói nhẹ"

#: builtin/push.c
